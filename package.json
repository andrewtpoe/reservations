{
  "name": "reservations",
  "version": "0.1.0",
  "description": "Reservations app written in node/express",
  "main": "index.js",
  "scripts": {
    "build:js": "browserify -t babelify src/client/browser.js -o src/public/app.js",
    "dev": "nodemon --debug",
    "start": "forever start index.js",
    "stop": "forever stop index.js",
    "test": "find ./src -name '*.test.js' | xargs mocha --require ./mocha-babel.js --recursive ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rpearce/reservations.git"
  },
  "author": "Robert Pearce <me@robertwpearce.com> (http://robertwpearce.com)",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/rpearce/reservations/issues"
  },
  "homepage": "https://github.com/rpearce/reservations#readme",
  "dependencies": {
    "babel": "^6.1.18",
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "db-migrate": "^0.9.23",
    "express": "^4.13.3",
    "forever": "^0.15.1",
    "history": "^1.13.1",
    "nodemon": "^1.8.1",
    "pg-promise": "^2.8.5",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-router": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "mocha": "^2.3.4"
  }
}
